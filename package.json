    "@nomiclabs/hardhat-ganache": "*",
    "@openzeppelin/contracts": "^4.6.0",
    "@trifle/leaderboard": "^1.0.8",
    "base64-sol": "^1.1.0",
    "chai": "^4.2.0",
    "dotenv": "^16.0.1",
    "eslint": "^9.20.1",
    "eslint-config-prettier": "^9.1.0",
